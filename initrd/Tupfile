: bin/busybox |> !tup_ln |> init
: $(files-y) $(files-m) |> ^ MKINITRD^ (for i in $(files-y) $(files-m); do echo $i; done) | cpio --quiet -o -H newc > %o |> initrd.cpio
: initrd.cpio |> ^ GZIP %o^ gzip -c %f > %o |> initrd
endif
